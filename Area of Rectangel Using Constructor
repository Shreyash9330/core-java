package basic;

public class Rectangle {

		int length;
		int width;
		
		
		Rectangle(int l,int w)
		{
			length =l;
			width =w;
		}
		
		void area()
		{
			int ar =length * width;
		 System.out.println("Area of Rectangle :"+ar);
		}
		
	

	public static void main(String[] args) {
		
		Rectangle rc = new Rectangle(26 ,8);

		rc.area();
	}

}
